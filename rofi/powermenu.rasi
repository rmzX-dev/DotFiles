configuration {
    font: "FiraCode Nerd Font 12";
    width: 200px;
    lines: 4;
    padding: 15;
    border-radius: 12px;
    background: #1e1e2e;
    foreground: #cdd6f4;
}

window {
    background-color: #1e1e2e;
    border: 2px solidrgb(250, 137, 137);
    border-radius: 12px;
}

element {
    padding: 8px;
    border-radius: 6px;
    text-color: #cdd6f4;
}

element selected {
    background-color: #89b4fa;
    text-color: #1e1e2e;
}